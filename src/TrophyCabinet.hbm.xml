<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
   <class name="gamesplayers.TrophyCabinet" table="tbl_TrophyCabinet">
      <meta attribute="class-description">
         This class contains trophy cabinet  details.
      </meta>
      <id name="trophyID" type="int" column="trophy_id">
         <generator class="increment"/>
      </id>
      <property name="trophyName" column="trophy_name" type="string" length="100"/>
      <property name="trophyDescription" column="trophy_desc" type="string"/>
      <property name="isTrophyUnlock" column="player_unlock" type="int" length="1"/>
   </class>
</hibernate-mapping>